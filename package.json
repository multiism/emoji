{
  "name": "emoji-renderer",
  "version": "0.1.0",
  "description": "Draw custom emoji procedurally",
  "repository": {
    "type": "git",
    "url": "https://github.com/multiism/emoji.git"
  },
  "bugs": {
    "url": "http://github.com/multiism/emoji/issues"
  },
  "author": "Isaiah Odhner <isaiahodhner@gmail.com>",
  "license": "MIT",
  "keywords": [
    "emoji",
    "expression",
    "facial-expression",
    "face",
    "faces",
    "smile",
    "smiley",
    "smileys",
    "smilies",
    "emotion",
    "emotive",
    "emotional",
    "emoticon",
    "sentiment",
    "human",
    "communication",
    "messaging",
    "chat"
  ],
  "main": "src/draw-emoji.js",
  "scripts": {
    "build": "webpack",
    "watch": "webpack --watch",
    "format": "prettier --write \"{*,src/**/*,docs/**/*}.{js,css}\""
  },
  "husky": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "{*,src/**/*,docs/**/*}.{js,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-loader": "8.0.6",
    "husky": "4.2.3",
    "lint-staged": "10.0.8",
    "prettier": "1.19.1",
    "webpack": "4.42.0",
    "webpack-cli": "3.3.12"
  },
  "dependencies": {
    "@babel/core": "7.8.7",
    "@babel/preset-env": "7.8.7"
  }
}
